{
  "name": "mintype",
  "version": "1.2.4",
  "description": "minimal type abstraction",
  "main": "index.js",
  "scripts": {
    "test:deps": "dependency-check . ",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test tape test/**/*.js",
    "test:coverage": "NODE_ENV=test nyc npm run test:node",
    "test:coverage:report": "nyc report --reporter=lcov npm run test:node",
    "test": "npm-run-all -s test:node test:lint test:deps"
  },
  "browserify": {
    "transform": [
      "bulkify",
      "es2040"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/mintype.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/mintype/issues"
  },
  "homepage": "https://github.com/ahdinosaur/mintype#readme",
  "engines": {
    "node": "^6.0.0"
  },
  "devDependencies": {
    "dependency-check": "^2.8.0",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "standard": "^8.6.0",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "bulk-require": "^1.0.0",
    "bulkify": "^1.4.2",
    "es2040": "^1.2.4",
    "object-assign": "^4.1.1"
